    <script  type="text/javascript">
        function affichecomm(elem){
            let idc = $(elem).attr('data-whatever');
            let url ="{{ path('commentaire_modif',{'id':'idcomment'})}}";
            url = url.replace("idcomment", idc);
            var contenue=$("#cont").val();
            var data='contenue='+contenue;
            $('#f').submit(function (e) {
                e.preventDefault();
                $.ajax({
                    type:"POST",
                    url:url,
                    data:data,
                    dataType: "json",
                    processData: true,

                });

            });

        }

        $("#close").on("click",function () {
            location.reload(true);
        });
        function affichierpub (elem)
        {


            var idc = $(elem).attr('data-whatever');
            var url ="{{ path('publication_show_one1',{'id_post':'idpub'})}}";
            url = url.replace("idpub", idc);
            $.ajax({
                type:"GET",
                url:url,
                dataType: "json",
                processData: true,
                success:function(data)
                {
                    var src1="/uploads/video/"+data['contenue'];
                    var src2="/uploads/img/"+data['contenue'];
                    $('#titlepub').html(data['titre']);
                    if(data['type'].localeCompare("video")==0)
                    {  $("#content").append('     <video  id=vid" class="video-fluid" autoplay loop muted>\n' +
                        '          <source src="" type="video/mp4" />\n' +
                        '        </video>');
                        document.getElementById('vid').src =src1;}
                    else{
                        $("#content").append('<img id="img" src="" class="img-thumbnail" alt="contenue" width="304" height="236">');
                        document.getElementById('img').src =src2;}




                }
            });
            $("#Posteditt").submit(function(){
                let posturl ="{{ path('publication_edit',{'id_post':'idpub'})}}";
                posturl =posturl.replace("idpub", idc);
                var form=$("#Posteditt")[0];
                $.ajax({
                    type:"POST",
                    url:posturl,
                    data:new FormData(form),
                    contentType:false,
                    processData:false,
                    cache:false,
                    dataType:"json",
                    error:function(err){
                        console.error(err);
                    },
                    success:function(data){
                        console.log(data);
                    },
                    complete:function(){
                        console.log("Request finished.");

                    }

                });

            });


        }
        var divClone = $("#content").clone();
        $("#fermm").on("click",function(){$("#content").replaceWith(divClone.clone());});


        var MaxChar = 255;
        var long= $('#textpublication').textContent().length;
        $('#compteur').html( long+' / ' + MaxChar );


    </script>
{% endblock %}